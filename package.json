{
  "name": "expotion-metal-calculator",
  "version": "0.2.0",
  "private": false,
  "type": "module",
  "description": "Калькулятор массы металлопроката на Next.js 15.5.7 (TS) с встроенными данными. Пересобран из Ringil Django-пакета под Next.",
  "license": "MIT",
  "homepage": "https://expotion.tech",
  "repository": {
    "type": "git",
    "url": "https://github.com/izumitellur/expotion-metal-calculator.git"
  },
  "author": "expotion",
  "keywords": [
    "metal",
    "calculator",
    "steel",
    "weight",
    "react",
    "next.js",
    "металлопрокат",
    "калькулятор"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "next dev",
    "build": "tsc -p tsconfig.build.json",
    "build:demo": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.5.7",
    "next": "15.5.7",
    "prettier": "^3.7.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "typescript": "5.9.2"
  }
}
